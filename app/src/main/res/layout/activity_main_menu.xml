<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainMenu">


    <TextView
        android:id="@+id/textView2"
        android:layout_width="157dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="99dp"
        android:layout_marginTop="14dp"
        android:text="Welcome"
        android:textAlignment="center"
        android:textSize="20dp" />

    <ImageView
        android:id="@+id/icoReadMeter"
        android:layout_width="95dp"
        android:layout_height="81dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="51dp"
        android:layout_marginTop="90dp"
        app:srcCompat="@drawable/meter" />

    <TextView
        android:id="@+id/textView42"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="63dp"
        android:layout_marginTop="179dp"
        android:text="Read Meter" />

    <TextView
        android:id="@+id/icoCheckSummary"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView42"
        android:layout_marginEnd="59dp"
        android:text="Check Summary" />

    <ImageView
        android:id="@+id/icoSummary"
        android:layout_width="95dp"
        android:layout_height="81dp"
        android:layout_alignStart="@+id/icoCheckSummary"
        android:layout_alignTop="@+id/icoReadMeter"
        app:srcCompat="@drawable/summary" />

    <ImageView
        android:id="@+id/icoUpload"
        android:layout_width="91dp"
        android:layout_height="81dp"
        android:layout_alignStart="@+id/icoReadMeter"
        android:layout_centerVertical="true"
        app:srcCompat="@drawable/upload" />

    <TextView
        android:id="@+id/Upload"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/textView42"
        android:layout_marginBottom="210dp"
        android:text="Upload Data" />

    <ImageView
        android:id="@+id/icoDownload"
        android:layout_width="95dp"
        android:layout_height="81dp"
        android:layout_alignStart="@+id/icoCheckSummary"
        android:layout_centerVertical="true"
        app:srcCompat="@drawable/download" />

    <TextView
        android:id="@+id/textView54"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/icoCheckSummary"
        android:layout_alignTop="@+id/Upload"
        android:text="Download Data" />

    <ImageView
        android:id="@+id/icoEditData"
        android:layout_width="95dp"
        android:layout_height="81dp"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/icoReadMeter"
        android:layout_marginBottom="86dp"
        app:srcCompat="@drawable/edit" />

    <TextView
        android:id="@+id/textView55"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="50dp"
        android:layout_marginStart="71dp"
        android:text="Edit Data" />

    <ImageView
        android:id="@+id/icoLogout"
        android:layout_width="95dp"
        android:layout_height="81dp"
        android:layout_alignStart="@+id/icoCheckSummary"
        android:layout_alignTop="@+id/icoEditData"
        app:srcCompat="@drawable/logout" />

    <TextView
        android:id="@+id/Log_Out"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView55"
        android:layout_marginEnd="96dp"
        android:text="Log Out" />

</RelativeLayout>